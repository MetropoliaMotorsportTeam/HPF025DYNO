import csv

# Read the input CSV file
with open('input.csv', 'r') as csvfile:
    reader = csv.reader(csvfile, delimiter=';')
    header_row = next(reader)  # Skip the header row if present
    with open('output.csv', 'w', newline='') as outfile:
        writer = csv.writer(outfile)
        for row in reader:
            processed_row = []
            for item in row:
                parts = item.rsplit(':', 1)
                if len(parts) == 2:
                    value, timestamp = parts[0].strip(), parts[1].strip()
                else:
                    # Handle cases where there's no colon or multiple colons without a timestamp
                    value = item.strip()
                    timestamp = ''  # Set to empty string or default value as needed
                processed_item = f"{timestamp},{value}"
                processed_row.append(processed_item)
            writer.writerow(processed_row)

print("Processing complete.")
